<HBox alignment="TOP_CENTER" minHeight="540.0" minWidth="720.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="group.seven.gui.MainMenu">
